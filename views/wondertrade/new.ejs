<% layout('../layout') -%>
<h2>Submit a new Wonder Trade</h2>

<form id="new-wondertrade" name="input" action="/wondertrade/new" method="post">
	<div class="input-columns">
		<div class="input-column">
            <div class="input-column-inner trainer-details">
                <h4>Trainer Information</h4>
                <div class="input-row">
                    Trainer Gender:
				<span class="radio-group">
					<label> Male &#9794; <input type="radio" name="gender" value="male" /></label>
					<label> Female &#9792; <input type="radio" name="gender" value="female" /></label>
				</span>
                </div>
                <div class="input-row">
                    <label>
                        <span class="required">Trainer Country:</span>
                        <select name="trainerCountry">
                            <option value="">Select a Country</option>
                            <% for(var country in countryList) { %>
                            <option value="<%= countryList[country]['id'] %>"><%= countryList[country].name %></option>
                            <% } %>
                        </select>
                    </label>
                </div>
                <div class="input-row">
                    <label>Trainer Country Sub: <input type="text" name="trainerCountrySub1" placeholder="New York, Tokyo, etc." /></label>
                </div>
                <p class="clarify-inputs">To clarify: These fields are for the<br/>trainer you received the wonder trade <u>from</u>, not yourself.</p>
            </div>
            <div class="input-column-inner ot-details">
                <h4>Original Trainer Information</h4>
                <div class="input-row">
                    <label>OT ID: <input type="text" name="trainerId" placeholder="01301" /></label>
                </div>
                <div class="input-row">
                    <label>OT Name: <input type="text" name="trainerName" placeholder="Ash" /></label>
                </div>
                <p class="clarify-inputs">
                    Important Note:<br/>
                    The OT ID and Name can be found on the Pokemon you received.<br/> (Not yourself.)
                </p>
            </div>

		</div>
		<div class="input-column pokemon-details">
            <div class="input-column-inner">
                <h4>Pokemon Received</h4>
                <div class="input-row">
                    <label>
                        <span class="required">Pokemon Received:</span>
                        <select name="pokemonId">
                            <%- include ../partials/_pokemon-select-menu.ejs %>
                        </select>
                    </label>
                </div>
                <div class="input-row">
                    <label><span class="required">Pokemon Level:</span> <input type="number" name="level" min="1" max="100" placeholder="1 - 100"  /></label>
                </div>
                <div class="input-row">
                    <label>
                        <span class="">Pokeball Type:</span>
                        <select name="pokeballType">
                            <option value=""></option>
                            <option value="pokeball">Poke Ball</option>
                            <option value="greatball">Great Ball</option>
                            <option value="ultraball">Ultra Ball</option>
                            <option value="masterball">Master Ball</option>
                            <option value="levelball">Level Ball</option>
                            <option value="lureball">Lure Ball</option>
                            <option value="moonball">Moon Ball</option>
                            <option value="friendball">Friend Ball</option>
                            <option value="loveball">Love Ball</option>
                            <option value="heavyball">Heavy Ball</option>
                            <option value="fastball">Fast Ball</option>
                            <option value="sportball">Sport Ball</option>
                            <option value="premierball">Premier Ball</option>
                            <option value="repeatball">Repeat Ball</option>
                            <option value="timerball">Timer Ball</option>
                            <option value="nestball">Nest Ball</option>
                            <option value="netball">Net Ball</option>
                            <option value="diveball">Dive Ball</option>
                            <option value="luxuryball">Luxury Ball</option>
                            <option value="healball">Heal Ball</option>
                            <option value="quickball">Quick Ball</option>
                            <option value="duskball">Dusk Ball</option>
                            <option value="dreamball">Dream Ball</option>
                            <option value="cherishball">Cherish Ball</option>
                            <option value="safariball">Safari Ball</option>
                            <option value="beastball">Beast Ball</option>
                        </select>
                    </label>
                </div>
                <div class="input-row checkbox-group">
                    <label><input type="checkbox" name="hasItem" />Item</label>
                    <label class="hiddenAbility-wrapper"><input type="checkbox" name="hasHiddenAbility" />Hidden Ability<div class="hiddenAbility-tooltip"></div></label>
                    <label><input type="checkbox" name="isShiny" />Shiny</label>
                    <label><input type="checkbox" name="hasPokerus" />Pokerus</label>
                    <label><input type="checkbox" name="hasEggMove" />Notable Egg Moves</label>
                    <label class="hasNotableIVLabel">
                        <input type="checkbox" name="hasPerfectIV" />Any Perfect IV
                    </label>
                    <p class="clarify-inputs">Perfect IV: Look for markings in the pokemon summary page, that usually means that the pokemon has perfect IVs. <a target="_blank" href="http://www.reddit.com/r/WonderTrade/comments/1q89p9/wonder_trade_psa_marking_ivs_xpost_from_rpokemon/">More Info</a></p>
                </div>
                <div class="input-row">
                    <label>Pokemon Nickname (if any): <input type="text" name="pokemonNickname" placeholder="Quaklin" /></label>
                </div>
                <div class="sentiment">
                    <div>Do you like the pokemon that you received?</div>
                    <label> Disliked <input type="radio" name="liked" value="dislike" /></label>
                    <label> Neutral <input type="radio" name="liked" value="" checked="checked" /></label>
                    <label> Liked <input type="radio" name="liked" value="like" /></label>
                </div>
            </div>
		</div>
	</div>
	<div class="requiredMessage">Trainer Country, Pokemon, and Pokemon Level are required fields.</div>
	<button><i class="pokeball-submit"></i>Submit</button>
</form>

